<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>                
    <link href="//maxcdn.bootstrapcdn.com/font-awesome/4.3.0/css/font-awesome.min.css" rel="stylesheet">
    <script type="text/javascript" src="~/js/jquery-1.9.1.min.js"></script>    
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/js/bootstrap.min.js" type="text/javascript"></script>        
    @RenderSection("additionalCSS", required: false)               
    <link rel="stylesheet" type="text/css" media="screen" href="~/css/mddmenu.css" />
    <link rel="stylesheet" type="text/css" media="screen" href="~/Content/Site.css" />
    <link rel="stylesheet" type="text/css" media="screen" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.6/css/bootstrap.min.css" />
</head>
<body>
    <div @{if (ViewBag.Ambiente != "MONOAZIENDALE"){<text>id="prebody"</text>}
           else
           {<text>id="prebodyMono"</text>}}>
    </div>
        <div id="intestazione">
         <div id="logo">	
            @{if (ViewBag.Ambiente != "MONOAZIENDALE")
              {<text>
                <img src="~/content/images/logo_main3.png"  />                
               </text>
              }
              else
              {<text>
                <img src="~/content/images/logo_mono_main.png" style="margin-top:8px" />                
              </text>
              }
              }
        </div>            
            @RenderSection("loggedUser", required: false)
            <br />
            </div>
            @RenderSection("navbar", required: false)    
            @RenderSection("breadCrumb", required: false)        
            @RenderSection("searchForm", required: false)                    
            @RenderSection("sectionTitle", required: false)    
    @RenderSection("scripts", required: false)           
    <div id="mainPanel">
    @RenderBody()
    </div>
<br /><br />
<div id="footer">
    <p>2018 <span class="min">®</span>Osserva srl</p>	    
</div>
        
</body>

</html>
