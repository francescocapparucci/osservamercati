<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>        
    <link rel="stylesheet" href="~/js/jquery.mobile-1.4.5.min.css" />
    <link rel="stylesheet" href="~/js/jquery.mobile.structure-1.4.5.min.css" />
    <link rel="stylesheet" href="~/js/jquery.mobile.theme-1.4.5.min.css" />
    <link rel="stylesheet" href="~/js/jquery.mobile.inline-png-1.4.5.min.css" />
    <link rel="stylesheet" href="~/js/jquery.mobile.external-png-1.4.5.min.css" />
    <link rel="stylesheet" href="~/js/jquery.mobile.icons-1.4.5.min.css" />
    
    <script src="~/js/jquery-1.11.0.min.js"></script>
    <script src="~/js/jquery.mobile-1.4.5.js"></script>    
    <script src="~/scripts/jquery-1.7.1.min.js" type="text/javascript"></script>
    <script src="~/js/jquery-ui-1.8.2.custom.min.js" type="text/javascript"></script>
    <script src="~/js/knockout-2.2.1.js"></script>
    <script src="~/js/globalize.min.js"></script>
    <script src="~/js/dx.chartjs.js"></script>  
    @RenderSection("additionalCSS", required: false)   
    <link rel="stylesheet" type="text/css" media="screen" href="~/Content/Mobile.css" />
</head>
<body>
    <div data-role="page" id="loggedHome">
    <div data-role="header" data-position="fixed" data-theme="a">    
        @RenderSection("sectionTitle", required: false)                      
    </div>    	        
	    @RenderSection("loggedUser", required: false)    
        <br />    
        @RenderSection("navbar", required: false)        
        @RenderSection("searchForm", required: false)                
        @RenderSection("scripts", required: false)       
    
    <div id="mainPanel" data-role="content">
        @RenderBody()
    </div>    
    <div data-role="footer" class="ui-bar ui-bar-a" data-theme="a" data-position="fixed">
	    <h1>2015 <span class="min">®</span>Osserva srl</h1>	    
    </div>    
    </div>
    <div id="myDialog" data-role="dialog" data-theme="a">
        <div data-role="header" data-theme="a">
            Ricerca Cerved<br /><br />
        </div>
        <div data-role="content" data-theme="a">
            Specificare Ragione Sociale oppure PartitaIva per ricerca
        </div>
    </div>
</body>
</html>
